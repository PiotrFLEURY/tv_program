{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build sources",
            "type": "shell",
            "command": "dart run build_runner build --delete-conflicting-outputs",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        },
        {
            "label": "Generate icons",
            "type": "shell",
            "command": "dart run flutter_launcher_icons",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        }
    ]
}